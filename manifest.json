{
  "name": "YTShorts_disable",
  "version": "1.0",
  "description": "Get rid of youtube shorts",
  "manifest_version": 3,
  "author": "Viroz",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "js": ["./src/scripts/youtube.js"],
      "matches": ["https://*.youtube.com/*"],
      "run_at": "document_end"
    }
  ]
}
